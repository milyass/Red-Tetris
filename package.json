  "dependencies": {
    "@reduxjs/toolkit": "^1.8.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.2.0",
    "cheet.js": "^0.3.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.5.0",
